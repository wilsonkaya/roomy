{{#each reviews as |review|}}
<div class="cardss card-1" data-id="{{review.id}}">
  <div class="room">
  </div>


  <label class="card-label">Comment:</label>
  <p id="super">{{review.comment}}</p>
  <label class="card-label">Rating:</label>
  <div class="star-location">
    <span class="rating-static rating-{{review.rating}}"></span>
  </div>
  <button type="button" class="btn btn-default update-myreview" data-id="{{review.id}}" name="button">Update review</button>
  <button type="button" class="btn btn-default delete-myreview" data-id="{{review.id}}" name="button">Delete review</button>


</div>

{{/each}}
